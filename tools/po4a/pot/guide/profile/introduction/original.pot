msgid "Software Developers"
msgstr ""

#. type: Plain text
#: guide/profile/introduction.adoc:50
msgid "More specifically, roles addressed are the following:"
msgstr ""

#. type: Plain text
#: guide/profile/introduction.adoc:52
msgid "ICT Architects"
msgstr ""

#. type: Plain text
#: guide/profile/introduction.adoc:53
msgid "ICT Developers"
msgstr ""

#. type: Plain text
#: guide/profile/introduction.adoc:54
msgid "Business Experts"
msgstr ""

